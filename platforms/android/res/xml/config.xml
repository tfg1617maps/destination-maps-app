<?xml version='1.0' encoding='utf-8'?>
<widget id="com.uc3m.tfgmaps" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="WikitudePlugin">
        <param name="android-package" value="com.wikitude.phonegap.WikitudePlugin" />
    </feature>
    <name>DestiMap</name>
    <description>
        UN SISTEMA WEB PARA CREAR MAPAS AUMENTADAS PARA EL TURISMO 2.0
    </description>
    <author email="100292677@alumnos.uc3m.es">
        Aar√≥n Laro de La Iglesia
    </author>
    <content src="index.html" />
    <icon src="res/map.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="portrait" />
</widget>
